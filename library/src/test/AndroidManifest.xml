<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.mobilejazz.android.diskcache.library">

    <application android:allowBackup="true"
                 android:label="abc"
        >

        <provider
            android:name="com.mobilejazz.android.diskcache.library.CacheProvider"
            android:authorities="@string/authority.cache"
            android:grantUriPermissions="true"
            android:exported="false">
            <meta-data
                android:name="android.support.FILE_PROVIDER_PATHS"
                android:resource="@xml/example_filepaths" />
            <meta-data
                android:name="com.mobilejazz.android.diskcache.META_MAX_AGE"
                android:value="1" />
            <meta-data
                android:name="com.mobilejazz.android.diskcache.META_MAX_SIZE"
                android:value="300" />
        </provider>

    </application>

</manifest>
